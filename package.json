{
    "name": "rxjs-conduit",
    "version": "1.0.0",
    "description": "A special kind of RxJS Subject that can be hot-spliced to other Conduits and holds updates for late subscribers.",
    "files": [
        "lib"
    ],
    "scripts": {
        "test": "node -r esbuild-register --test **/*.spec.ts"
    },
    "author": "Fasteroid",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fasteroid/rxjs-conduit.git"
    },
    "keywords": [
        "typescript",
        "rxjs",
        "reactive",
        "observable",
        "declarative",
        "angular"
    ],
    "bugs": {
        "url": "https://github.com/Fasteroid/rxjs-conduit/issues"
    },
    "homepage": "https://github.com/Fasteroid/rxjs-conduit#readme",
    "devDependencies": {
        "@types/node": "^20.17.6",
        "esbuild-register": "^3.6.0",
        "typescript": "^5.6.3",
        "@angular/core": ">=16.0.0"
    },
    "peerDependencies": {
        "rxjs": ">=6.6.7",
        "@angular/core": ">=16.0.0"
    },
    "peerDependenciesMeta": {
        "@angular/core": {
            "optional": true
        }
    },
    "exports": {
        "./vanilla": {
            "types": "./lib/vanilla/index.d.ts",
            "default": "./lib/vanilla/index.js"
        },
        "./angular": {
            "types": "./lib/angular/index.d.ts",
            "default": "./lib/angular/index.js"
        }
    }
}
